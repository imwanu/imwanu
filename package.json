{
  "name": "io.cordova.hellocordova",
  "displayName": "HelloCordova",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "cordova": {
    "platforms": [
      "android",
      "ios"
    ],
    "plugins": {
      "cordova-plugin-device": {},
      "cordova-plugin-contacts": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-ios-contacts-permissions": {
        "CONTACTS_USAGE_DESCRIPTION": "允許存取聯絡人"
      },
      "cordova-plugin-geolocation": {
        "GEOLOCATION_USAGE_DESCRIPTION": "App uses geolocation to determine where you are."
      },
      "cordova-plugin-device-orientation": {},
      "cordova-plugin-qr-barcode-scanner": {
        "ANDROID_SUPPORT_V4_VERSION": "27.+"
      }
    }
  },
  "devDependencies": {
    "cordova": "^12.0.0",
    "cordova-android": "^13.0.0",
    "cordova-ios": "^7.1.1",
    "cordova-plugin-add-swift-support": "^2.0.2",
    "cordova-plugin-contacts": "^3.0.1",
    "cordova-plugin-device": "^3.0.0",
    "cordova-plugin-device-orientation": "^3.0.0",
    "cordova-plugin-geolocation": "^5.0.0",
    "cordova-plugin-qr-barcode-scanner": "^8.0.3",
    "cordova-plugin-statusbar": "^4.0.0"
  },
  "dependencies": {
    "cordova-plugin-ios-contacts-permissions": "1.0.0"
  }
}